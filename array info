#include <iostream>
using namespace std;

void printarr(int arr[], int size){
    
    cout<< "The array is" <<endl;
    
    for(int i=0; i<size; i++){
        
        cout<< arr[i]<< " ";
    }
}



int main() {
	// your code goes here
	
	int first[5]= {2,1};
	
	printarr(first,5);
	
	
	int sizeofarray = sizeof(first)/sizeof(int);
	cout<< sizeofarray << endl;
	
	




	return 0;
}

