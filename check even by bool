#include <iostream>
using namespace std;

bool iseven(int a){
    
    if(a&1){
        return 0;
        
    }
    else{
        return 1;
    }
}


int main() {
	// your code goes here
	
	int n;
	cin>>n;
	
	if(iseven(n)){
	    cout<<"EVEN"<<endl;
	    
	}
	else{
	    cout<<"ODD"<<endl;
	    
	}


	return 0;
}
