#include <iostream>
#include<climits>
using namespace std;

void reverse(int arr[], int n){
    
    int start=0;
    int end=(n-1);
    
    while(start<=end){
        swap(arr[start],arr[end]);
        start++;
        end--;
        
    }
}


int main() {
	// your code goes here
	int err[10]={3,5,2,23,42,765,7,56,86,54};
	
	reverse(err,10);
	
	cout<< err[2] <<endl;
	
	

	return 0;
}
