// array has n elements where n=2m+1 , thus it has m elements which are twice , and 1 unique element , you need to find that

#include <iostream>
#include<climits>
using namespace std;

int unique(int arr[], int n){
    int X=0;
    
    for(int i=0;i<n;i++){
        
        X=X^arr[i];
        
        
    }
    return X;
    
}


int main() {
	// your code goes here
	int err[5]={2,5,7,5,2};
	
	cout<<unique(err,5)<<endl;
	


	return 0;
}
