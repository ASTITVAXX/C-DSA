//pattern 1 

#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int n,X;
	cin>>n;
	
	for(int i=0;i<4;i++){
	    for(int j=0;j<4;j++){
	        cout<<"*";
	        
	    }
	    cout<<endl;
	   
	    
	}
	return 0;
}






// pattern 2 
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int n=1,X;

	
	for(int i=0;i<4;i++){
	    for(int j=0;j<4;j++){
	        cout<<n;
	        
	    }
	    n=n+1;
	    
	    cout<<endl;
	   
	    
	}
	return 0;
}

// pattern 3 
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int n=1,X;

	
	for(int i=0;i<3;i++){
	    for(int j=1;j<4;j++){
	        cout<<j;
	        
	    }
	  
	    
	    cout<<endl;
	   
	    
	}
	return 0;
}


// pattern 4 
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int n=1,X;

	
	for(int i=1;i<5;i++){
	    for(int j=0;j<i;j++){
	        cout<<"#";
	        
	    }
	  
	    
	    cout<<endl;
	   
	    
	}
	return 0;
}

// pattern 5

#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int n,X;
	cin>>n;

	
	for(int i=1;i<=n;i++){
	    for(int j=1;j<=i;j++){
	        cout<<(i-j+1);
	        
	    }
	  
	    
	    cout<<endl;
	   
	    
	}
	return 0;
}

