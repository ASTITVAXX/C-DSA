#include<iostream>
#include<climits>
#include<bits/stdc++.h>
using namespace std;



int main(){

int AR[6]={12,6,21,10,19,4};

for(int i=1;i<6;i++){
    
    for(int j=0;j<6-i;j++){

        if(AR[j+1]<AR[j]){
             swap(AR[j+1],AR[j]);

        }
    }

    

}

cout<<AR[1]<<endl;



    return 0;

}

