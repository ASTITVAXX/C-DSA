#include <iostream>
using namespace std;



int main() {
	// your code goes here
	
	int a, b;
	char op;
	
	cout<<"Enter a"<<endl;
	cin>>a;
	
	cout<<"Enter b"<<endl;
	cin>>b;
	
	cout<<"enter the operation"<<endl;
	cin>>op;
	
	
	switch(op){
	    
	    case '+' : cout<<(a+b)<<endl;
	    break;
	    
	    case '-' : cout<<(a-b)<<endl;
	    break;
	    
	    case '*' : cout<<(a*b)<<endl;
	    break;
	    
	    case '/' : cout<<(a/b)<<endl;
	    break;
	    
 
	    
	}

	return 0;
}
