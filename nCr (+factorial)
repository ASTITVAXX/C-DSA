#include <iostream>
using namespace std;


int Fac(int n){
    if(n==0){
        return 1;
    }
    else{
    for(int i=(n-1);i>0;i--){
        n=n*i;
        
    }
    return n;
    }
    
}


int nCr(int a,int b){
    
    return Fac(a)/(Fac(b) * Fac(a-b));
    
}

int main() {
	// your code goes here
	
	cout<<nCr(4,3)<<endl;

	return 0;
}
