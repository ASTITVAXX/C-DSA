#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int n,X;
	cin>>n;
	
	for(int i=2;i<n;i++){
	    
	    if(n%i !=0){
	        
	    }
	    else{
	        
	        cout<<"Given n is not prime"<<endl;
	        break;}
	    X=i;
	    	if(n%X !=0){cout<<"n is prime"<<endl;}
	}

	
	return 0;
}
