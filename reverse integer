#include <iostream>
using namespace std;

int pof2(int a){
    int i=1;
    int N=2;
    while(i!=(a)){
        
       
        N=N*2;
        i=i+1;
        
        
    }
    
    return N;
    
    
    
}

int main() {
	// your code goes here
	int n,X;
	 int Z=0;
	int i =10;
	
	int MAX= (pof2(31))-1, MIN= -(pof2(31));
	
	cin>>n;
	
    while(n/i !=0){
	   i=i*10;
	   
	    
	    
	}
	
	X=i/10;
	
	cout<<X<<endl;
	
	
	while(n!=0){
	   int Y= n%10;
	   n=n/10;
	   
	   
	  
	   Z=Z+(Y*X);
	   X=X/10;
	   
	   
	}
	
	
	if(Z>MAX || Z<(-pof2(31))){
	    cout<<"ERROR"<<endl;
	    
	    return 0;
	    
	}else{cout<<Z<<endl;}
	
	return 0;
}
